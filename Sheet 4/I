#include<stdio.h>
#include<string.h>
int main()
{
    int i,j,count=0;
    char s[1003];
    scanf("%s",&s);
    for( i=0,j=strlen(s)-1;i<strlen(s),j>=0;i++,j--)
    {
        if(s[j]!= s[i]){

            count++;
            break;
        }
    }
    if(count==0)
    {
        printf("YES\n");

    }
    else

        printf("NO\n");
    return 0;
}
