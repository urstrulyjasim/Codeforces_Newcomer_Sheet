#include<stdio.h>
int main()
{
    long long N,i,j,sum=0;
    scanf("%lld",&N);
    long long a[N];
    for(int i=0;i<N;i++)
    {
        scanf("%lld",&a[i]);
        sum=sum+a[i];
    }
    if(sum>=0)
    {
        printf("%lld\n",sum);
    }
    else if(sum<0){
        sum = sum*(-1);
        printf("%lld\n",sum);
    }

    return 0;
}
