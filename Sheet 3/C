#include<stdio.h>
int main()
{
    int N;
    scanf("%d",&N);
    int a[N];
    for(int i=0;i<N;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]>0)
        {
            a[i]=1;
        }
        if(a[i]<0)
        {
            a[i]=2;
        }
    }
    for(int i=0;i<N;i++)
    {
        printf("%d ",a[i]);
    }
    printf("\n");
    return 0;
}
