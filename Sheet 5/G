//بِسْمِ اللهِ الرَّحْمٰنِ الرَّحِيْمِ
#include<bits/stdc++.h>
using namespace std;
void fun(int t)
{
    long long a[t];
    for(int i=0;i<t;i++)
    {
        cin>>a[i];
    }
    long long min = a[0];
    long long max = a[0];
    for(int i=1;i<t;i++)
    {
        if(a[i]>max)
        {
            max = a[i];

        }
        if(a[i]<min)
        {
            min = a[i];
        }
    }
    cout<<min<<" "<<max<<endl;
}       
int main()
{
    int t;
    cin>>t;
    fun(t);      
    
    return 0;
}
