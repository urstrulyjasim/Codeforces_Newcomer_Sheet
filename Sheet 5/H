//بِسْمِ اللهِ الرَّحْمٰنِ الرَّحِيْمِ
#include<bits/stdc++.h>
using namespace std;
void show (int t)
{
    int n;
    char ch;
    while(t--)
    {
        cin>>n>>ch;
        for(int i=1;i<=n;i++)
        {
            cout<< ch<<" ";
        }
        cout<<endl;
    }
}       
int main()
{
    int t;
    cin>>t;
    show(t);
    return 0;
}
