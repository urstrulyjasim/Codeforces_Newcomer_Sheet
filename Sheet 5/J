//بِسْمِ اللهِ الرَّحْمٰنِ الرَّحِيْمِ
#include<bits/stdc++.h>
using namespace std;
#define maxn 10000
double a[maxn];
void sum(int n)
{
    double sum = 0;
    for(int i=0;i<n;i++)
    {
        sum +=a[i];
    }
    double avg = sum/n;
    cout<<fixed<<setprecision(7);
    cout<<avg;
}
int main()
{
    double n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }      
    sum(n);

    
    return 0;
}
