#include<stdio.h>
int main()
{
    int N,rem,sum=0;
    scanf("%d",&N);
    int temp=N;
    while(temp!=0){
        rem=temp%10;
        sum = sum*10+rem;
        temp=temp/10;
    }
    printf("%d\n",sum);
    if(sum==N){
        printf("YES\n");
    }
    else
        printf("NO\n");
    return 0;
}
