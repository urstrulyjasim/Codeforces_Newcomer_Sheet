#include<stdio.h>
int main()
{
    int N,p=1;
    scanf("%d",&N);
    for(int i=1; i<=N; i++)
    {
        for(int j=1; j<=4; j++)
        {
            if(p%4!=0)
            {
                printf("%d ",p);
            }
            p++;
        }
        printf("PUM\n");
    }
    return 0;
}
