//even, odd, positive and negative.
#include<stdio.h>
int main()
{
    int N;
    int a,even=0,odd=0,pos=0,neg=0;
    scanf("%d",&N);
    for(int i=1; i<=N; i++)
    {
        scanf("%d",&a);
        if(a%2==0)
        {
            even++;
        }
        if(a%2==1 || a%2==-1)
        {
            odd++;
        }
        if(a>0)
        {
            pos++;
        }
        if(a<0)
        {
            neg++;
        }
    }

    printf("Even: %d\n",even);
    printf("Odd: %d\n",odd);
    printf("Positive: %d\n",pos);
    printf("Negative: %d\n",neg);


    return 0;
}
