#include<stdio.h>
int main()
{
    int num1,num2,max,min;
    long long sum=0;
    while(1){
        scanf("%d %d",&num1,&num2);
        if(num1<=0 || num2<=0){
            return 0;
        }
        else{
        if(num1>=num2){
            max=num1;
            min=num2;
        }
        else{
            max=num2;
            min=num1;
        }


       for( int i=min;i<=max;i++){
          sum=sum+i;
          printf("%d ",i);
       }
       printf("sum =%lld\n",sum);
        }
      sum=0;
    }



    return 0;
}
