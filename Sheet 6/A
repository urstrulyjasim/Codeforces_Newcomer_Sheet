//بِسْمِ اللهِ الرَّحْمٰنِ الرَّحِيْمِ
#include<bits/stdc++.h>
using namespace std;
void power(long long n)
{
    long long flag =0;
    while(n!=1)
    {
        if(n%2!=0)
        {
            flag++;
        }
        n/=2;
        
    }
    if(flag==0)
    {
        cout<<"YES"<<endl;
    }
    else
        cout<<"NO"<<endl;
}
       
int main()
{
    long long n;
    cin>>n;
    power(n)      ;
    
    return 0;
}
